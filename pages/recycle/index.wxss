.container {
}

.recycle-hd {
    background: #fff;
    border-top: 1px solid #ececec;
    border-bottom: 1px solid #ececec;
    height: 45px;
    overflow: hidden;
}

.recycle-bd {
    margin-top: 10px;
    background: #fff;
}

.recycle-category-list {
}

.recycle-cagegory-item {
    padding: 10px;
}

.recycle-cagegory-item .recycle-cagegory-img {
    height: 50px;
    width: 100%;
    overflow: hidden;
    text-align: center;
}

.recycle-cagegory-item .recycle-cagegory-img image {
    width: 50px;
    height: 50px;
}

.recycle-cagegory-title {
    text-align: center;
    margin-top: 10px;
}

.recycle-category-nodata {
    height: 50px;
    text-align: center;
    line-height: 50px;
}

.recycle-category-table {
    margin: 15px 10px;
    min-height: 100px;
    border-bottom: 1px solid #ccc;
    border-top: 1px solid #ccc;
}

.recycle-category-table .row {
    clear: both;
    height: 30px;
    border-bottom: 1px solid #ccc;
    border-top: 1px solid #ccc;
}

.recycle-category-table .row:first-child {
    border-top-width: 0;
}

.recycle-category-table .row:last-child {
    border-bottom-width: 0;
}

.recycle-category-table .row:nth-child(even) {
    background: #ececec;
}

.recycle-category-table .col {
    border-left: 1px solid #ccc;
    border-right: 1px solid #ccc;
    height: 30px;
    line-height: 30px;
}

.recycle-category-table .row:nth-child(even) {
    border-bottom-width: 0;
    border-top-width: 0;
}

.recycle-category-table .col:nth-child(even) {
    border-left-width: 0;
}

.recycle-category-table .category-type {
    width: 30%;
    float: left;
    box-sizing: border-box;
    text-align: center;
}

.recycle-category-table .category-price {
    width: 70%;
    float: left;
    box-sizing: border-box;
    text-align: center;
}

.recycle-ft {
    padding: 10px;
    clear: both;
}